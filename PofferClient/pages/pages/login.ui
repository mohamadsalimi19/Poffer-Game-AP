/* لیبل‌ها متن سفید و فونت کمی بزرگ‌تر */
QLabel {
    color: #ffffff;
    font-weight: 600;
    font-size: 16px;
}

/* دکمه‌ها با رنگ پس‌زمینه خاکستری تیره و متن سفید */
QPushButton {
    background-color: #2c2c2c;
    color: #ffffff;
    border: 1.5px solid #4caf50; /* حاشیه سبز */
    border-radius: 8px;
    padding: 8px 16px;
    font-weight: bold;
    transition: background-color 0.3s ease;
}

QPushButton:hover {
    background-color: #4caf50;  /* سبز روشن */
    color: #000000;             /* متن مشکی */
}

/* ورودی متن */
QLineEdit {
    background-color: #1a1a1a;
    color: #e0e0e0;
    border: 1px solid #4caf50;
    border-radius: 6px;
    padding: 6px 10px;
}

QLineEdit:hover {
    border-color: #81c784;  /* سبز روشن‌تر */
}

/* انتخاب متن در ورودی */
QLineEdit:selected {
    background-color: #4caf50;
    color: #000000;
}
